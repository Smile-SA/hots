{
    "analysis": {
      "window_duration": "33",
      "sep_time": "33"
    },
    "clustering": {
      "algo": "kmeans",
      "nb_clusters": 3
    },
    "data": {
      "individuals_file": "container_usage.csv",
      "hosts_meta_file": "node_meta.csv",
      "individual_field": "container_id",
      "host_field": "machine_id",
      "tick_field": "timestamp",
      "metrics": ["cpu"]
    },
    "heuristic": {
      "algo": "distant_pairwise"
    },
    "optimization": {
      "model": "src/cots/model_small_pyomo.py",
      "solver": "glpk"
    },
    "loop": {
      "mode": "event",
      "tick": "default",
      "constraints_dual": ["mustLink", "nb_clusters"],
      "tol_dual_clust": 0.15,
      "tol_move_clust": 1,
      "tol_dual_place": 0.15,
      "tol_move_place": 1,
      "tol_step": 0.0
    },
    "plot": {
      "renderer": "chromium"
    },
    "allocation":{
      "enable": false,
      "constraints":{
        "load_threshold": 0.5,
        "max_amplitude": 0.5
      },
      "objective":{
        "open_nodes": 1,
        "target_load_CPU": 0.8 
      }
    },
    "placement":{
      "enable": true
    }
  }
  
